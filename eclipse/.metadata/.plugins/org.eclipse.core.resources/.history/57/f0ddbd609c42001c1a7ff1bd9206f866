package znick_.biomesabroad.biome.pirateshores.deco.block;

import net.minecraft.block.Block;
import net.minecraft.block.material.Material;
import znick_.biomesabroad.BiomesAbroad;

public class RedMushroomBlock extends Block {

	public RedMushroomBlock() {
		super(Material.wood);
		this.setBlockName("red_mushroom_top");
		this.setBlockTextureName(BiomesAbroad.MODID + ":biomes/mushroom_forest/red_mushroom_block");
	}
}
